    {
      "name": "0198f05e",
      "displayName": "New Query 2025-09-03 1:04am (Forecasted)",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `houses_pricing`;\n",
        "\n",
        "WITH original_table AS (\n",
        "    SELECT \n",
        "  DATE_TRUNC(\"MONTH\", `Date_Sold`) AS Date_Sold,\n",
        "  SUM(`Price`) AS Price\n",
        "FROM (\n",
        "    SELECT\n",
        "  datesold AS Date_Sold,\n",
        "  TRIM(postcode) AS Region_Postcode,\n",
        "  TRIM(propertyType) AS Property_Type,\n",
        "  bedrooms AS Number_Of_Bedrooms,\n",
        "  price As Price,\n",
        "  YEAR(Date_Sold) As Year_DATE,\n",
        "  month(Date_Sold) AS Month_Name,\n",
        "  weekday(Date_Sold) AS Day_Name,\n",
        "  price / NULLIF(bedrooms, 0) As Average_Price_per_Bedroom\n",
        "from\n",
        "  raw_data\n",
        ") GROUP BY 1\n",
        "  ),\n",
        "  dates AS (\n",
        "    SELECT \n",
        "      MAX(Date_Sold) AS max_date_column,\n",
        "      MIN(Date_Sold) AS min_date_column,\n",
        "      MAX_BY(Price, Date_Sold) AS Price\n",
        "    FROM original_table\n",
        "  ),\n",
        "  forecast_table AS (\n",
        "    SELECT \n",
        "      Date_Sold,\n",
        "      Price_forecast,\n",
        "      Price_upper,\n",
        "      Price_lower,\n",
        "      NULL AS Price\n",
        "    FROM AI_FORECAST(\n",
        "      TABLE(original_table),\n",
        "      horizon => (\n",
        "        SELECT\n",
        "          max_date_column + MAKE_DT_INTERVAL(CAST(FLOOR(DATEDIFF(max_date_column, min_date_column) * 0.5) AS INT), 0, 0, 0)\n",
        "        FROM dates\n",
        "      ),\n",
        "      time_col => 'Date_Sold',\n",
        "      value_col => 'Price'\n",
        "    )\n",
        "  )\n",
        "\n",
        "  SELECT * FROM forecast_table\n",
        "  UNION ALL\n",
        "  SELECT\n",
        "    Date_Sold,\n",
        "    NULL AS Price_forecast,\n",
        "    NULL AS Price_upper,\n",
        "    NULL AS Price_lower,\n",
        "    Price\n",
        "  FROM original_table\n",
        "    WHERE Date_Sold < (SELECT max_date_column FROM dates)\n",
        "  UNION ALL\n",
        "  SELECT\n",
        "    max_date_column AS Date_Sold,\n",
        "    Price AS Price_forecast,\n",
        "    Price AS Price_upper,\n",
        "    Price AS Price_lower,\n",
        "    Price\n",
        "  FROM\n",
        "    dates"
      ],
      "catalog": "workspace",
      "schema": "houses_pricing"
    }
  ],
